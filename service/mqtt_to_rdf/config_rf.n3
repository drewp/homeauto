@prefix fr: <http://bigasterisk.com/foaf/> .

:wallButton1 a :MqttStatementSource;
  :mqttTopic ("rfsetup" "rf_received");
  :filterPayloadJsonEquals "{\"protocol\":1,\"code0\":0,\"code1\":7906}";
